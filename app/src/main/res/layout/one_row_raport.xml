<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/raportrow">

    <TextView
        android:id="@+id/raportdata"
        android:layout_width="98dp"
        android:layout_height="24dp"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/raportimie" />

    <TextView
        android:id="@+id/raportocena"
        android:layout_width="53dp"
        android:layout_height="24dp"
        android:layout_marginStart="14dp"
        android:layout_marginLeft="14dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/raportdata"
        app:layout_constraintTop_toBottomOf="@+id/raportnazwisko" />

    <TextView
        android:id="@+id/raportprzedmiot"
        android:layout_width="110dp"
        android:layout_height="24dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/raportnazwisko"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/raportnazwisko"
        android:layout_width="82dp"
        android:layout_height="24dp"
        android:layout_marginStart="22dp"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="20dp"
        android:text="Nazwisko"
        app:layout_constraintStart_toEndOf="@+id/raportimie"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/raportimie"
        android:layout_width="90dp"
        android:layout_height="24dp"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="20dp"
        android:text="ImiÄ™"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/raportinfo"
        android:layout_width="162dp"
        android:layout_height="24dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/raportocena"
        app:layout_constraintTop_toBottomOf="@+id/raportprzedmiot" />
</androidx.constraintlayout.widget.ConstraintLayout>